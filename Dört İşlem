
public class Main {
	
	public static void main(String args[])
	{
		
		int a=48,b=12,topla,fark,böl,çarp;
		topla=a+b;
		fark=a-b;
		böl=a/b;
		çarp=a*b;
		System.out.println("Toplam = " + (a+b));
		System.out.println("Fark = " + (a-b));
		System.out.println("Çarpım = " + (a*b));
		System.out.print("Bölüm = " + (a/b));
	}

}
